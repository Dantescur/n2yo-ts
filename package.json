{
  "name": "n2yo-ts",
  "version": "0.1.4",
  "private": false,
  "packageManager": "pnpm@10.13.1",
  "description": "A strongly-typed TypeScript client for the N2YO.com satellite tracking API.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/Dantescur/n2yo-ts#readme",
  "bugs": {
    "url": "https://github.com/Dantescur/n2yo-ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dantescur/n2yo-ts.git"
  },
  "author": "Daniel <cesardaniel.9611@gmail.com>",
  "files": [
    "README.md",
    "dist/**/*"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "n2yo-ts": "./dist/cli.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown && rm -f ./dist/cli.cjs ./dist/cli.d.cts ./dist/cli.d.ts",
    "dev": "tsdown --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "typecheck": "tsc --noEmit",
    "format": "prettier --cache --write .",
    "release": "pnpm test && pnpm format && pnpm lint:fix && pnpm typecheck && git add . && git commit -m 'chore: format and lint fixes' || true && bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "commander": "^14.0.0",
    "prompts": "^2.4.2",
    "zod": "^4.0.11"
  },
  "devDependencies": {
    "@arethetypeswrong/core": "^0.18.2",
    "@sxzz/eslint-config": "^7.0.6",
    "@sxzz/prettier-config": "^2.2.3",
    "@types/node": "^24.1.0",
    "@types/prompts": "^2.4.9",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "bumpp": "^10.2.0",
    "eslint": "^9.32.0",
    "prettier": "^3.6.2",
    "publint": "^0.3.12",
    "tsdown": "^0.13.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "unplugin-unused": "^0.5.1",
    "vitest": "^3.2.4",
    "vitest-fetch-mock": "^0.4.5"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "prettier": "@sxzz/prettier-config"
}
